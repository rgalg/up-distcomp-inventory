sudo -i -u postgres
psql
CREATE DATABASE up_distcomp_inventory_db;
CREATE USER up_distcomp_inventory_user WITH ENCRYPTED PASSWORD '<password here>';
GRANT ALL PRIVILEGES ON DATABASE up_distcomp_inventory_db TO up_distcomp_inventory_user;
\q
exit


sudo -u postgres psql -d up_distcomp_inventory_db -f postgres-config/complete_setup.sql

chmod +x setup_pgsql_db.sh
./setup_pgsql_db.sh
